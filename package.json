{
  "name": "listd",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel vite typesafe-i18n",
    "vite": "vite dev --open",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint --ext .js,.cjs,.ts,.svelte . --fix",
    "format": "prettier --plugin-search-dir . --write .",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull",
    "prisma:studio": "npx prisma studio",
    "prepare": "husky install",
    "typesafe-i18n": "typesafe-i18n"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@skeletonlabs/skeleton": "^0.92.2",
    "@sveltejs/adapter-auto": "^1.0.3",
    "@sveltejs/kit": "^1.30.3",
    "@tailwindcss/forms": "^0.5.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.35.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "postcss-load-config": "^4.0.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^4.16.2",
    "svelte": "^3.59.2",
    "svelte-check": "^3.6.3",
    "svelte-eslint-parser": "^0.24.2",
    "svelte-preprocess": "^4.10.7",
    "tabler-icons-svelte": "^1.10.0",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typesafe-i18n": "^5.26.2",
    "typescript": "^4.9.5",
    "vite": "^4.5.2",
    "vitest": "^0.25.8"
  },
  "type": "module",
  "license": "MIT",
  "engines": {
    "npm": "Please use pnpm instead of npm.",
    "yarn": "Please use pnpm instead of yarn.",
    "pnpm": ">=7.0.0"
  },
  "bugs": {
    "url": "https://github.com/jbabin91/listd/issues"
  },
  "author": "Jace Babin",
  "repository": {
    "type": "git",
    "url": "git@github.com:jbabin91/listd.git"
  },
  "keywords": [
    "listd",
    "svelte",
    "typescript",
    "tailwindcss"
  ]
}
