{
  "name": "listd",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "env:sync": "sync-dotenv",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull",
    "prisma:studio": "npx prisma studio",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.0",
    "@skeletonlabs/skeleton": "^0.92.2",
    "@sveltejs/adapter-auto": "^1.0.3",
    "@sveltejs/kit": "^1.13.0",
    "@tailwindcss/forms": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.6",
    "prettier-plugin-svelte": "^2.10.0",
    "prisma": "^4.11.0",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "svelte-preprocess": "^4.10.7",
    "sync-dotenv": "^2.7.0",
    "tabler-icons-svelte": "^1.10.0",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vitest": "^0.25.8"
  },
  "type": "module",
  "license": "MIT",
  "engines": {
    "npm": "Please use pnpm instead of npm.",
    "yarn": "Please use pnpm instead of yarn.",
    "pnpm": ">=7.0.0"
  },
  "bugs": {
    "url": "https://github.com/jbabin91/listd/issues"
  },
  "author": "Jace Babin",
  "repository": {
    "type": "git",
    "url": "git@github.com:jbabin91/listd.git"
  },
  "keywords": [
    "listd",
    "svelte",
    "typescript",
    "tailwindcss"
  ]
}
